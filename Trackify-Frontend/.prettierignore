# Ignore build artifacts
/build/
/public/

# Ignore archive files
*.zip
*.tar.gz

# Ignore dependency folders
/node_modules/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE configurations
/.vscode/
/.idea/
/*.code-workspace

# Ignore output and temporary files
/dist/
/out/
*.tmp
*.cache
.DS_Store

# Ignore test coverage reports
/coverage/

# Ignore generated assets
*.min.js
*.min.css
